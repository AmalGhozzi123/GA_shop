<script id="cmt-tracking" type="text/javascript">
    var merchant = "518002949";
    var language = "en-gb";
    var currency = "gbp";
    var currencySymbol = "Â£";

    /**
     * CMT utilities
     */
    var CMT = {
                events: {
          MINI_CART_UPDATED: "add_to_cart.mini_cart_updated",
          INSTANT_SEARCH_SHOW_RESULT: "instant_search.show_result"
        },
        merchant: merchant,
        language: language,
        currency: currency,
        currencySymbol: currencySymbol,

        /**
         * Get the cart lines and clean the array, send data to SF
         */
        getCart: function() {
            var identifier = 'data-sandwatch-cart-lines';
            var element = document.querySelector('[' + identifier + ']');
            var hasData = element && element.getAttribute(identifier);
            var cartData = [];

            if (hasData) {
                try { cartData = JSON.parse(element.getAttribute(identifier)) || [];
                } catch (e) { cartData = []; }
                cartData = cartData.map(function(item) {
                    if (item.model) {
                        item.unique_id = item.item;
                        delete item.model;
                    }
                    return item;
                });
                return cartData.length > 0 ? { cart: cartData } : { clear_cart: true };
            }
            return { clear_cart: true };
        },

        /**
         * Get the category identifier if any
         */
        getCategory: function() {
            var identifier = 'data-category-identifier';
            var element = document.querySelector('[' + identifier + ']');

            return element && element.getAttribute(identifier) ?
                { category: element.getAttribute(identifier) } : null;
        },

        /**
         * Get the product identifier if any
         */
        getProduct: function() {
            var identifier = 'data-product-id';
            var element = document.querySelector('body[data-page="page-product"] [' + identifier + ']');

            return element && element.getAttribute(identifier) ?
                { item: element.getAttribute(identifier) } : null;
        },

        /**
         * Get the search term if any
         */
        getSearch: function() {
            var identifier = 'data-search-result-query';
            var element = document.querySelector('body[data-page="page-search-results"] [' + identifier + ']');

            return element && element.getAttribute(identifier) ?
                { search: element.getAttribute(identifier) } : null;
        },

        /**
         * Get the user id if any
         */
        getUser: function() {
            var identifier = 'data-sandwatch-user-customer-id-gtm';
            var element = document.querySelector('[' + identifier + ']');

                        return element && element.getAttribute(identifier) ?
                { email: element.getAttribute(identifier) } : null;
        },

        /**
         * Track the page view with all the info needed at once
         * to merge in a unique message.
         */
        getTrackPageViewData: function() {
            var trackPageViewObject = {};
            ['getCategory', 'getSearch', 'getProduct'].forEach(function(func) {
                var data = CMT[func]();
                if (data !== null) {
                  for (var [key, value] of Object.entries(data)) {
                    trackPageViewObject[key] = value;
                  }
                }
            });
            return trackPageViewObject;
        },

        /**
         * Populate the global client object with all the known properties to cover
         */
        collectData: function(filter) {
            var map = {
                'orgId': ['setOrgId', CMT.merchant],
                'user': ['setUserInfo', CMT.getUser()],
                'page': ['trackPageView', CMT.getTrackPageViewData()],
            };

            var filteredKeys = !filter ?
                Object.keys(map) :
                Object.keys(map).filter(function (el) { return filter.indexOf(el) !== -1});

            filteredKeys.forEach(function(key) {
                var data = map[key];
                CMT.pushData(data[0], data[1]);
            });
        },

        /**
         * Tracking of Cart addition events
         */
        sendCart: function() {
            CMT.pushData('trackCart', CMT.getCart());
        },

        /**
         * Tracking of Instant Search events
         */
        sendInstantSearch: function(terms) {
            CMT.pushData('trackPageView', { search: terms });
        },

        /**
         * CMT tracking Init
         */
        send: function() {
            // Init the CMT data push object
            this.collectData();

            // Register all the CMT recommendation modules in page
            this.registerAllModules();
        },

        /**
         * Send data to Salesforce creating a push object
         * with a key and a value
         */
        pushData: function(key, data) {
            if (key && data !== null) {
                CMT.push([key, data]);
            }
        },

        /**
         * Push directly to Salesforce
         */
        push: function(data) {
            var _etmc = window._etmc || [];
            if (data) {
                _etmc.push(data);
            }
        },

        /** MODULES FOR RECOMMENDATIONS ****/

        /**
         * Register modules in page
         */
        registerAllModules: function registerAllModules() {
            var elements = document.querySelectorAll('[data-cmt-module]');
            var list = ['containerId', 'templateId', 'logic', 'limit'];
            var readAttr = function(element, name) {
                return element.getAttribute('data-cmt-' + name);
            };
            var addProp = function(collection, prop, value) {
                if (value && value !== "") collection[prop] = value;
                return collection;
            };
            [].forEach.call(elements, function(element) {
                var message = list.reduce(function(message, prop) {
                    return addProp(message, prop, readAttr(element, prop));
                }, {});
                //CMT.pushData("recommend", message);
            });
        },

        /**
         * Helper for CMT data extraction
         */
        $t: function(data, variable) {
            return data[variable + "_" + CMT.language] || null;
        },

        /**
         * Helper to print product price
         */
        printPrice: function(price){
            return this.currencySymbol + ' ' + parseFloat(price).toLocaleString(this.language, {minimumFractionDigits: 2, maximumFractionDigits: 2});
        },
    };

    /**
     * Register and Manage custom tracking events listener
     */
    (function() {
        if (window.document && window.document.addEventListener) {
            window.addEventListener('ux-refactor-loaded', function(e) {
                if (typeof e.detail === 'object') {
                    var engine = e.detail;

                    /* AddToCart Event
                     * Sent by CoC via backend
                     */
                    /*engine.Events.subscribeForEvent(
                        CMT.events.MINI_CART_UPDATED,
                        function (data) {
                            CMT.sendCart();
                        }
                    );*/

                    // Instant Search Event
                    engine.Events.subscribeForEvent(
                        CMT.events.INSTANT_SEARCH_SHOW_RESULT,
                        function (data) {
                            if (data && data.terms) {
                                CMT.sendInstantSearch(data.terms);
                            }
                        }
                    );
                }
            });
        }
    })();
</script>
</body>
</html>
